<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/ic_go_back"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="36dp"

            android:src="@drawable/ic_go_back"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.04" />

        <TextView
            android:id="@+id/my_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.1"
            android:textSize="20dp"
            android:textColor="#000000"
            android:textStyle="bold"
            android:text="@string/my_page"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/ic_chulsu"
            android:layout_width="99dp"
            android:layout_height="99dp"
            android:src="@drawable/img_chulsu"
            android:layout_marginStart="36dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.16" />

        <LinearLayout
            android:id="@+id/ll_type"
            android:layout_width="80dp"
            android:layout_height="28dp"
            android:layout_marginTop="110dp"
            android:background="@drawable/bg_type_in_dialog"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:orientation="horizontal"/>

        <EditText
            android:id="@+id/chulsu_info"
            android:layout_width="200dp"
            android:layout_height="48dp"
            android:background="@drawable/all_edge"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.22"
            android:layout_marginStart="171dp"
            android:layout_marginEnd="36dp"
            />

        <TextView
            android:id="@+id/mypage_specific_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.3"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:text="@string/mypage_specific_info"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/mypage_specific_info_2"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="@drawable/all_edge"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.38"
            android:layout_marginStart="36dp"
            android:layout_marginEnd="36dp"
            />

        <TextView
            android:id="@+id/percentage_of_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.48"
            android:textSize="20dp"
            android:textColor="#000000"
            android:textStyle="bold"
            android:text="@string/percentage"
            tools:ignore="MissingConstraints" />

        <com.github.mikephil.charting.charts.PieChart
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:id="@+id/piechart"
            android:name="com.naver.maps.map.MapFragment"
            android:layout_marginStart="36dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.7"
            tools:ignore="MissingClass"/>

        <ImageView
            android:id="@+id/ic_call"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:src="@drawable/img_mypage_call"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.86" />
        <ImageView
            android:id="@+id/ic_qna"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:src="@drawable/img_mypage_qna"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.93" />
        <ImageView
            android:id="@+id/ic_setting"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:src="@drawable/img_mypage_setting"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1" />



    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>